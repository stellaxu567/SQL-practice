#1
SELECT id, title
FROM movie
WHERE yr=1962

#2
SELECT yr
FROM movie
WHERE title='Citizen Kane'

#3
SELECT id,title, yr
FROM movie
WHERE title LIKE '%Star Trek%'
ORDER BY yr

#4
SELECT id 
FROM actor
WHERE name= 'Glenn Close'

#5
SELECT id 
FROM movie 
WHERE title='Casablanca'

#6#
SELECT name
FROM actor, casting
WHERE movieid=(SELECT id FROM movie
WHERE movieid=11768 AND title='Casablanca')
AND actorid=actor.id

#7
SELECT name
FROM actor, casting
WHERE movieid=(select id FROM movie WHERE title='Alien')
AND id=actorid

#8#
SELECT title
FROM movie, actor, casting
WHERE name like '%Harrison Ford%'
AND casting.actorid=actor.id
AND casting.movieid=movie.id

#9
SELECT title
FROM movie, actor, casting
WHERE name like '%Harrison Ford%'
AND ord<>1
AND movieid=movie.id
AND actorid=actor.id

#10#
SELECT title, name
FROM movie, casting, actor
WHERE ord=1
AND yr=1962
AND movie.id=movieid
AND actor.id=actorid
